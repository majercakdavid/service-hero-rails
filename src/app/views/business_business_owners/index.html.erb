<p id="notice"><%= notice %></p>

<h1>Business Business Owners</h1>

<table>
  <thead>
    <tr>
      <th>Business</th>
      <th>Business owner</th>
      <th>Date from</th>
      <th>Date to</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_business_owners.each do |business_business_owner| %>
      <tr>
        <td><%= business_business_owner.business %></td>
        <td><%= business_business_owner.business_owner %></td>
        <td><%= business_business_owner.date_from %></td>
        <td><%= business_business_owner.date_to %></td>
        <td><%= link_to 'Show', business_business_owner %></td>
        <td><%= link_to 'Edit', edit_business_business_owner_path(business_business_owner) %></td>
        <td><%= link_to 'Destroy', business_business_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

